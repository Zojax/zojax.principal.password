#: src/zojax/principal/password/default.py:27
msgid "Password min length exceeded."
msgstr "Minimale lengte van het wachtwoord is bereikt"

#: src/zojax/principal/password/default.py:39
#: src/zojax/principal/password/interfaces.py:107
msgid "Password should contain both letters and digits."
msgstr "Wachtwoord moet letters en cijfers bevatten."

#: src/zojax/principal/password/default.py:43
#: src/zojax/principal/password/interfaces.py:113
msgid "Password should contain letters in mixed case."
msgstr "Wachtwoord moet letters en hoofdletters bevatten."

#: src/zojax/principal/password/interfaces.py:100
msgid "Minimum length"
msgstr "Minimale lengte"

#: src/zojax/principal/password/interfaces.py:101
msgid "Minimun length of password."
msgstr "Minimale lengte van het wachtwoord."

#: src/zojax/principal/password/interfaces.py:106
msgid "Letters and digits"
msgstr "Letters en cijfers"

#: src/zojax/principal/password/interfaces.py:112
msgid "Letters case"
msgstr "Letters"

#: src/zojax/principal/password/interfaces.py:46
msgid "Password manager"
msgstr "Wachtwoordbeheer"

#: src/zojax/principal/password/interfaces.py:52
